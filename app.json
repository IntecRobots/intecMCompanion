{
  "expo": {
    "name": "intecMCompanion",
    "slug": "intecMCompanion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/RobotLogoBig.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fnv457.intecMCompanion"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/RobotLogoBig.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fnv457.intecMCompanion"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.320328755483-7or0e1fcrs54h7rp9mh4hq4h3qmg3q21"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "363e1fd2-d2b3-4870-a2c9-e84a18d04281"
      }
    }
  }
}
